// original file: Z2Z4-AdditiveCodes
// Example: H2E8()
print "Example: H2E8";
ei := GetEchoInput();
SetEchoInput(true);

R := RSpace(IntegerRing(4), 6);
C := Z2Z4AdditiveCode([R![2,2,1,1,3,1], R![2,2,2,2,2,2], 
                       R![0,0,1,1,1,3]], 2);
Z2Z4Type(C);
#C;
InformationRate(C);

SetEchoInput(ei);
